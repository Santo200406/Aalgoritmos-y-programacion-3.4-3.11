Algoritmo Ejercicio_3_8
    // Definir variables
    Definir A, B, C Como Real
    Definir res_a, res_b, res_c Como Real
	
    // Asignar valores dados
    A <- 4
    B <- 5
    C <- 1
	
    // a) B * A - (B^2) / 4 * C
    res_a <- B * A - (B ^ 2) / 4 * C
	
    // b) (A * B) / 3^2
    res_b <- (A * B) / (3 ^ 2)
	
    // c) (((B + C) / 2 * A + 10) * 3 * B) - 6
    res_c <- (((B + C) / 2 * A + 10) * 3 * B) - 6
	
    // Mostrar resultados
    Escribir "A =", A, " B =", B, " C =", C
    Escribir "Resultado a) =", res_a
    Escribir "Resultado b) =", res_b
    Escribir "Resultado c) =", res_c

FinAlgoritmo
